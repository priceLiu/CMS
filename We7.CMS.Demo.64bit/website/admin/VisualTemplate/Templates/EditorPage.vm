<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    ${head}
    <script src="/Admin/VisualTemplate/Scripts/LoadJquery.js" type="text/javascript"></script>
    <link href="/Scripts/jQuery/jQueryUI/1.8.5/css/dark-hive/jquery-ui-1.8.5.custom.css" rel="stylesheet" type="text/css">
    <script src="/Scripts/jQuery/jQueryUI/1.8.5/js/jquery-ui-1.8.5.custom.min.js" type="text/javascript"></script>
    <link href="/Admin/VisualTemplate/Style/VisualDesign.LayoutsBasics.css" rel="stylesheet" type="text/css">
    <link href="/Admin/VisualTemplate/Style/VisualDesign.Base.css" rel="stylesheet" type="text/css" />
    <script src="/Scripts/Common.js" type="text/javascript"></script>
    <script src="/Admin/VisualTemplate/Scripts/TemplateBuilder.js" type="text/javascript"></script>
    <script src="/Admin/VisualTemplate/Scripts/VisualDesign.js" type="text/javascript"></script>
    <script src="/Admin/VisualTemplate/Scripts/VisualDesign_new.js" type="text/javascript"></script>
    <script src="/Admin/VisualTemplate/Scripts/WidgetProperty.js" type="text/javascript"></script>
    <script src="/Admin/VisualTemplate/Scripts/writeCapture.js" type="text/javascript"></script>

</head>
<body class="sfPageEditor zeContentMode" activate="*" style="cursor: auto;">
    <div class="sfPageWrapper">
        <div id="ZoneEditorToolboxContainer" class="ZoneEditorToolboxContainer">
            <ul>
                <li><a href="#zeToolboxContainer" id="widgetTab">控件</a></li>
                <li><a href="#zeLayoutboxContainer" id="layouttab">布局</a></li>
            </ul>
            <div id="zeToolboxContainer" class="zeToolboxContainer" style="padding: 0; margin: 0;">
            </div>
            <div id="zeLayoutboxContainer" class="zeLayoutboxContainer">
                <ul style="list-style: none;">
                    <li class="layoutitem rdTitleBar" data='{name:"Column1"}'>
                    <img src="images/layout/1.png"/>
                    <span>100%</span>
                    </li>
                     <li class="layoutitem rdTitleBar" data='{name:"Column2-1"}'>
                      <img src="images/layout/2.png"/>
                    <span>75%+25%</span>
                    </li>
                      <li class="layoutitem" data='{name:"Column2-2"}'>
                       <img src="images/layout/3.png"/>
                    <span>25%+75%</span>
                    </li>
                      <li class="layoutitem" data='{name:"Column2-3"}'>
                       <img src="images/layout/4.png"/>
                    <span>50%+50%</span>
                    </li>
                      <li class="layoutitem" data='{name:"Column3-1"}'>
                       <img src="images/layout/5.png"/>
                    <span>25%+25%+50%</span>
                    </li>
                      <li class="layoutitem" data='{name:"Column3-2"}'>
                       <img src="images/layout/7.png"/>
                    <span>33%*3</span>
                    </li>
                      <li class="layoutitem" data='{name:"Column4-1"}'>
                       <img src="images/layout/9.png"/>
                    <span>25%*4</span>
                    </li>
                      <li class="layoutitem" data='{name:"Column5-1"}'>
                       <img src="images/layout/10.png"/>
                    <span>20%*5</span>
                    </li>
                </ul>
            </div>
        </div>
        <div class="sfPageContainer" id="sfPageContainer">
            <div class="sfPublicWrapper">
                <div style="min-width: 10px; min-height: 10px;" placeholderid="Body" class="RadDockZone RadDockZone_Sitefinity rdVertical "
                    id="RadDockZoneBody">
                    ${body}
                </div>
            </div>
        </div>
        <div class="clear"></div>
    </div>
</body>
</html>
